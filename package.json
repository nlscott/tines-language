{
  "name": "tines-language",
  "displayName": "tines-language",
  "description": "syntax highlighting for tines formulas",
  "version": "0.1.0",
  "icon": "images/icon.png",
  "publisher": "NicScott",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tines",
      "aliases": ["tines", "tines"],
      "extensions": ["tines"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tines",
      "scopeName": "source.tines",
      "path": "./syntaxes/tines.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nlscott/tines-language"
  },
  "homepage": "https://github.com/nlscott/tines-language"
}
